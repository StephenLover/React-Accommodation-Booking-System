    "jw-react-pagination": "^1.0.7",
    "mongoose": "^5.2.15",
    "react": "^16.5.2",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-rating": "^1.4.0",
    "react-router-dom": "^4.3.1",
    "react-slideshow-image": "^1.0.1",
